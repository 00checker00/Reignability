{
    "compilerOptions": {
        "target": "es2016",
        "module": "es2015",
        "lib": [
            "dom",
            "es2015",
            "es2016"
        ],
        "moduleResolution": "node",
        "sourceMap": true,
        "removeComments": false,
        "preserveConstEnums": true,
        "skipLibCheck": true,
        "typeRoots": ["./node_modules/@types", "./_build/@types","./types"],
        "experimentalDecorators": true,
    }
}